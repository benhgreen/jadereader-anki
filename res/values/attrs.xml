<resources>
    <declare-styleable name="ExpandableListView">
        <attr name="bar_height" format="dimension"/>
        <attr name="close_button_width" format="dimension"/>
        <attr name="background_color" format="reference|color"/>
        <attr name="drag_color" format="reference|color"/>
        <attr name="initial_height" format="dimension"/>
        <attr name="base_text_size" format="dimension|integer"/>
        <attr name="show_bar" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="MenuBar">
        <attr name="divider_color" format="reference|color"/>
        <attr name="button_background" format="reference|color"/>
        <attr name="button_background_on_pressed" format="reference|color"/>
        <attr name="button_text_color" format="reference|color"/>
        <attr name="title_size" format="dimension"/>
        <attr name="show_text" format="boolean"/>
        <attr name="button_position">
            <enum name="left" value="1"/>
            <enum name="right" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="FileItemView">
        <attr name="checkedColor" format="reference|color"/>
        <attr name="uncheckedColor" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="Slider">
        <attr name="text_background" format="reference|color"/>
        <attr name="text_color" format="reference|color"/>
        <attr name="max" format="integer"/>
    </declare-styleable>
</resources>
